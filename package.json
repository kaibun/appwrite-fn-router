{
  "name": "@kaibun/appwrite-fn-router",
  "version": "0.1.0",
  "description": "A router for Appwrite Functions",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./types": {
      "import": "./types/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "workspaces": [
    "functions/*"
  ],
  "scripts": {
    "build": "tsup",
    "doc:build": "cd doc && npm run build",
    "doc:start": "cd doc && npm run start -- --port 3001",
    "format": "prettier --write .",
    "sync": "npm run build && npm run build --prefix functions/Test",
    "test": "appwrite run functions --function-id 6890bbb20003ff2a1bf1",
    "dev:run": "npm run test",
    "dev:watch": "nodemon",
    "dev": "npm run sync && npm-run-all --parallel dev:watch dev:run",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "itty-router": "^5.0.21",
    "node-appwrite": "^14.2.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/node": "^24.1.0",
    "cypress": "^14.5.3",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-loader": "^9.5.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.9",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.9.2"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true
  }
}
