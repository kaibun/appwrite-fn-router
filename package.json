{
  "name": "@kaibun/appwrite-fn-router",
  "version": "0.1.0",
  "description": "A router for Appwrite Functions",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "doc:build": "cd doc && npm run build",
    "doc:start": "cd doc && npm run start -- --port 3001",
    "format": "prettier --write .",
    "sync": "mkdir -p functions/Test/src/lib && cp -r src/* functions/Test/src/lib/ && cp tsconfig.base.json functions/Test/",
    "test": "npm run sync && appwrite run functions --function-id 6890bbb20003ff2a1bf1",
    "postinstall": "cd doc && npm install"
  },
  "dependencies": {
    "itty-router": "^5.0.21",
    "node-appwrite": "^14.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/node": "^24.1.0",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.9",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.9.2"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true
  }
}
