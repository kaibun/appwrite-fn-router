{
  "name": "@kaibun/appwrite-fn-router",
  "version": "0.3.1",
  "description": "A router for Appwrite Functions",
  "repository": "github:kaibun/appwrite-fn-router",
  "homepage": "https://appwrite-fn-router.kaibun.net",
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "workspaces": [
    "functions/*"
  ],
  "scripts": {
    "check-hygiene": "node scripts/check-project-hygiene.cjs",
    "build": "tsup",
    "doc:build": "cd doc && npm run build",
    "doc:start": "cd doc && npm run start -- --port 3001",
    "format": "prettier --write .",
    "sync": "npm run build && npm run build --prefix functions/Test",
    "function": "cross-env NODE_ENV=development appwrite run functions --function-id 6890bbb20003ff2a1bf1",
    "dev:run": "npm run function",
    "dev:watch": "cross-env NODE_ENV=development nodemon",
    "dev": "docker ps >/dev/null 2>&1 || { echo '‚ùå Docker is not running.'; exit 1; }; npm run sync && npm-run-all --parallel dev:watch dev:run",
    "test": "npm-run-all --aggregate-output --continue-on-error --parallel test:unit test:e2e",
    "test:unit": "cross-env NODE_ENV=test vitest run --config vitest.config.ts",
    "test:e2e": "cross-env NODE_ENV=test cypress run"
  },
  "dependencies": {
    "itty-router": "^5.0.21",
    "node-appwrite": "^14.2.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.8.1",
    "@types/node": "^24.1.0",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.3",
    "docusaurus-plugin-typedoc": "^1.4.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "ts-loader": "^9.5.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.9",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "tsup": {
    "entry": [
      "src/main.ts",
      "types/index.d.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true
  }
}
