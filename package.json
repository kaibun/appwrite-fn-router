{
  "name": "@kaibun/appwrite-fn-router",
  "version": "0.1.0",
  "description": "A router for Appwrite Functions",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "sync": "cp -r src/* functions/Test/src/lib/",
    "test": "appwrite run functions --function-id 6890bbb20003ff2a1bf1"
  },
  "dependencies": {
    "itty-router": "^5.0.21",
    "node-appwrite": "^14.2.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0"
  },
  "tsup": {
    "entry": [
      "src/main.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true
  }
}
