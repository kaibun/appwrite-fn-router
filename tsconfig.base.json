{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "declaration": true,
    "declarationDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "lib": ["ES2022"],
    "module": "es2022",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@kaibun/appwrite-fn-router": ["./src/main.ts"],
      "@kaibun/appwrite-fn-router/types": ["./types/index.d.ts"],
      "@/*": ["./*"]
    },
    "noEmit": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "useUnknownInCatchVariables": true
  },
  "include": ["./src", "./types"]
}
